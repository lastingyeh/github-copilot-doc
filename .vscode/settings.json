{
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "image": false,
    "readme": false
  },
  "files.associations": {
    ".env*": "plaintext",
    ".tmp": "plaintext",
    ".png": "image"
  },
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.compile.nullAnalysis.mode": "disabled"
}
